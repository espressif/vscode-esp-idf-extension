          "idf.portWin": {
            "type": "string",
            "default": "COM1",
            "description": "%param.port%",
            "scope": "resource"
          },
          "idf.pythonBinPath": {
            "type": "string",
            "default": "python",
            "description": "%param.pythonBinPath%",
            "scope": "resource"
          },
          "idf.pythonBinPathWin": {
            "type": "string",
            "default": "python",
            "description": "%param.pythonBinPath%",
            "scope": "resource"
