{
    "env": {
        "amd": true,
        "browser": true
    },
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "no-console": "off"
    },
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [
                    ".js"
                ]
            }
        }
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2015
    }
}